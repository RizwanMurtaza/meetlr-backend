{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost;Port=3306;Database=Meetlr;User=root;Password=;"
  },
  "JwtSettings": {
    "SecretKey": "",
    "Issuer": "CalendlyCloneApi",
    "Audience": "CalendlyCloneUsers",
    "ExpiryMinutes": "60",
    "RefreshTokenExpiryDays": "7"
  },
  "Stripe": {
    "SecretKey": "",
    "PublishableKey": "",
    "ClientId": "",
    "ApplicationFeePercent": "10",
    "WebhookSecret": ""
  },
  "Google": {
    "ClientId": "",
    "ClientSecret": ""
  },
  "Microsoft": {
    "ClientId": "",
    "ClientSecret": "",
    "TenantId": ""
  },
  "VideoConferencing": {
    "Zoom": {
      "ClientId": "",
      "ClientSecret": "",
      "RedirectUri": "http://localhost:3000/calendar/callback"
    },
    "Slack": {
      "ClientId": "",
      "ClientSecret": "",
      "RedirectUri": "http://localhost:3000/calendar/callback"
    },
    "Jitsi": {
      "ServerUrl": "https://meet.jit.si",
      "RoomPrefix": "meetlr"
    }
  },
  "OAuth": {
    "Google": {
      "RedirectUri": "http://localhost:3000/auth/oauth/callback"
    },
    "Microsoft": {
      "RedirectUri": "http://localhost:3000/auth/oauth/callback"
    }
  },
  "RecurringBookings": {
    "MaxOccurrences": 10,
    "CalendarBatchSize": 10,
    "CalendarBatchDelayMs": 150,
    "MaxRetryAttempts": 3
  },
  "ApplicationUrls": {
    "BaseUrl": "http://localhost:3000",
    "FrontendUrl": "http://localhost:3000",
    "ApiUrl": "http://localhost:5000"
  },
  "ExternalApis": {
    "Google": {
      "CalendarScope": "https://www.googleapis.com/auth/calendar",
      "EmailScope": "https://www.googleapis.com/auth/userinfo.email",
      "ProfileScope": "https://www.googleapis.com/auth/userinfo.profile",
      "UserInfoEndpoint": "https://www.googleapis.com/oauth2/v2/userinfo"
    },
    "Microsoft": {
      "Authority": "https://login.microsoftonline.com/common",
      "CalendarScope": "https://graph.microsoft.com/Calendars.ReadWrite",
      "EmailScope": "https://graph.microsoft.com/User.Read",
      "UserInfoEndpoint": "https://graph.microsoft.com/v1.0/me"
    },
    "Stripe": {
      "ConnectUrl": "https://connect.stripe.com/oauth/authorize"
    }
  },
  "Cache": {
    "Enabled": true,
    "DefaultDurationSeconds": 300,
    "Provider": "Memory",
    "RedisConnectionString": null,
    "KeyPrefix": "meetlr",
    "MemoryCacheMaxEntries": 1000
  },
  "FileStorage": {
    "Provider": "Auto",
    "MaxFileSizeBytes": 10485760,
    "AllowedExtensions": [".jpg", ".jpeg", ".png", ".gif", ".webp", ".svg", ".ico"],
    "S3": {
      "AccessKeyId": "",
      "SecretAccessKey": "",
      "BucketName": "",
      "Region": "us-east-1",
      "ServiceUrl": "",
      "CdnUrl": "",
      "ForcePathStyle": false
    },
    "Local": {
      "StoragePath": "uploads",
      "BaseUrl": "http://localhost:5000/uploads"
    }
  }
}